modules = ["nodejs-20", "web", "postgresql-16"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-25_05"

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8000
externalPort = 8000

[[ports]]
localPort = 38577
externalPort = 5173

[[ports]]
localPort = 38817
externalPort = 6800

[[ports]]
localPort = 40359
externalPort = 6000

[[ports]]
localPort = 42185
externalPort = 3001

[[ports]]
localPort = 42369
externalPort = 4200

[[ports]]
localPort = 43191
externalPort = 5000

[[ports]]
localPort = 43427
externalPort = 3000

[[ports]]
localPort = 44091
externalPort = 3002

[[ports]]
localPort = 44665
externalPort = 3003

[env]
PORT = "5000"

[agent]
integrations = ["javascript_openai:1.0.0", "javascript_mem_db:1.0.0", "javascript_log_in_with_replit:1.0.0"]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Admin Panel"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[[workflows.workflow]]
name = "Admin Panel"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd admin-app && npx vite"
waitForPort = 8000

[workflows.workflow.metadata]
outputType = "console"
